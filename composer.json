{
    "name": "sanmai/sdk-common",
    "description": "Common types and contracts for PHP API SDKs",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com",
            "homepage": "https://www.alexeykopytko.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "doctrine/collections": "^1.8 ||^2.3",
        "guzzlehttp/guzzle": "^6.5.8 || ^7.9.2",
        "psr/http-message": "^2.0",
        "sanmai/json-serializer": "^0.1.1 || ^0.2.6",
        "sanmai/pipeline": "^5.2.1 || ^6.12"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.45",
        "friendsofphp/php-cs-fixer": "^3.73.1",
        "infection/infection": "^0.29.9",
        "phan/phan": "^5.4.5",
        "php-coveralls/php-coveralls": "^2.7.0",
        "phpstan/phpstan": "^1.12.21",
        "phpunit/phpunit": "^9.6.22",
        "psr/log": "^1.1.4",
        "symfony/http-foundation": "^6.4.18 || ^7.2.3",
        "vimeo/psalm": "^6.7.0"
    },
    "autoload": {
        "psr-4": {
            "CommonSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CommonSDK\\Tests\\Common\\": "tests/Common/",
            "Tests\\CommonSDK\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.5.x-dev"
        }
    }
}
