{
    "name": "sanmai/sdk-common",
    "type": "library",
    "description": "Common types and contracts for PHP API SDKs",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com",
            "homepage": "https://www.alexeykopytko.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*",
        "doctrine/collections": "^1.6",
        "guzzlehttp/guzzle": "^6.3 || ^7.2",
        "psr/http-message": "^1.0.1",
        "sanmai/json-serializer": "^0.1.1 || ^0.2.2",
        "sanmai/pipeline": "^5 || ^6"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.8.2",
        "friendsofphp/php-cs-fixer": "^2.16.4",
        "gamez/psr-testlogger": "^3.0",
        "infection/infection": ">=0.18",
        "phan/phan": "^3.2.3 || ^4.0.0 || ^5.0.0",
        "php-coveralls/php-coveralls": "^2.4.1",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.4.1",
        "psr/log": "^1.1.3",
        "symfony/http-foundation": "^4.4.7 || ^5.1.7",
        "vimeo/psalm": "^3.17.2 || ^4.1.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.5.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "CommonSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CommonSDK\\Tests\\Common\\": "tests/Common/",
            "Tests\\CommonSDK\\": "tests/"
        }
    }
}
